{
  "manifest_version": 2,
 
  "name": "MAXimize",
  "version": ".1.0",
  "description": "A browser extension that transforms all the images you see into photos of Max Nikias.",

  	"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html",
	"default_title": "MAXimize"
	},
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://*/*", "http://*/*"]
  }]
 
}